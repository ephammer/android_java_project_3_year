<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.binyamin.android5778_0445_7734_01.controller.MainActivity">


    <Button
        android:id="@+id/addClientbutton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Add Client" />

    <Button
        android:id="@+id/addModelCar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=" Add Model Car" />

    <Button
        android:id="@+id/addBranch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=" Add Branch" />
    <Button
        android:id="@+id/displayListClientbutton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Display List Client" />

    <Button
        android:id="@+id/displayListCarmodel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Display All Car Model" />
    <Button
        android:id="@+id/displayListBranch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Display All Branches" />

</LinearLayout>
